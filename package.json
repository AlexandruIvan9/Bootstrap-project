    "bootstrap": "^4.6.0",
    "bootstrap-social": "^5.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.6.0",
    "popper.js": "^1.16.1",
    "tippy.js": "^6.3.1",
    "tooltip.js": "^1.3.3"
  },
